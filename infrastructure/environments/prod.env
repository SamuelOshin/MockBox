# Production Environment Configuration
NODE_ENV=production
NEXT_PUBLIC_API_URL=https://api.mockbox.dev

# These are set via GitHub Secrets in CI/CD
NEXT_PUBLIC_SUPABASE_URL=${PROD_SUPABASE_URL}
NEXT_PUBLIC_SUPABASE_ANON_KEY=${PROD_SUPABASE_ANON_KEY}

# Production-specific settings
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true
NEXT_PUBLIC_ENVIRONMENT=production

# Performance optimizations
NEXT_PUBLIC_CACHE_TTL=3600
NEXT_PUBLIC_CDN_URL=https://cdn.mockbox.dev
